      "source": [
        "def findprimenumbers(startnum, endnum):\r\n",
        "  primenumber = []\r\n",
        "  for x in range(startnum,endnum+1):\r\n",
        "    if x > 1:\r\n",
        "      for y in range(2, x):\r\n",
        "        if x % y == 0:\r\n",
        "          break\r\n",
        "      else:\r\n",
        "        primenumber.append(x)\r\n",
        "  return primenumber"
      ],
      "execution_count": 1,
      "outputs": []
    },
